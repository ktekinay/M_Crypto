#tag Class
Protected Class EncrypterTestGroup
Inherits TestGroup
	#tag Event
		Sub TearDown()
		  if SimultaneousThread isa Thread then
		    
		    IsTimeToDie = true
		    
		    while SimultaneousThread isa Thread and SimultaneousThread.State <> Thread.NotRunning
		      App.YieldToNextThread
		    wend
		    
		    RemoveHandler SimultaneousThread.Run, WeakAddressOf SimultaneousThread_Run
		    SimultaneousThread = nil
		  end if
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub CloneDuringStreamingTest()
		  dim e as M_Crypto.Encrypter = GetEncrypter( "password" )
		  
		  if e is nil then
		    return
		  end if
		  
		  e.SetInitialVector "1234567890123456"
		  
		  dim data as string = kLongData.LeftB( 64 )
		  
		  dim expected as string = e.EncryptCBC( data )
		  
		  //
		  // Get it streaming
		  //
		  call e.EncryptCBC( data, false )
		  
		  //
		  // Get the clone
		  //
		  dim clone as M_Crypto.Encrypter = CloneEncrypter( e )
		  
		  //
		  // Compare it
		  //
		  dim actual as string = clone.EncryptCBC( data )
		  Assert.AreEqual EncodeHex( expected ), EncodeHex( actual )
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h1
		Protected Function CloneEncrypter(e As M_Crypto.Encrypter) As M_Crypto.Encrypter
		  dim ti as Xojo.Introspection.TypeInfo = Xojo.Introspection.GetType( e )
		  dim c as Xojo.Introspection.ConstructorInfo
		  for each test as Xojo.Introspection.ConstructorInfo in ti.Constructors
		    dim params() as Xojo.Introspection.ParameterInfo = test.Parameters
		    if params.Ubound = 0 and params( 0 ).ParameterType = GetTypeInfo( M_Crypto.Encrypter ) then
		      c = test
		      exit
		    end if
		  next
		  
		  if c is nil then
		    return nil
		  end if
		  
		  dim params() as Auto
		  params.Append e
		  dim clone as M_Crypto.Encrypter = c.Invoke( params )
		  
		  return clone
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub CloneTest()
		  dim e as M_Crypto.Encrypter = GetEncrypter( "password" )
		  
		  if e is nil then
		    return
		  end if
		  
		  e.UseFunction = M_Crypto.Encrypter.Functions.CBC
		  
		  dim clone as M_Crypto.Encrypter = CloneEncrypter( e )
		  
		  dim data as string = kLongData.LeftB( 64 )
		  
		  Assert.AreEqual clone.Decrypt( e.Encrypt( data ) ), e.Decrypt( clone.Encrypt( data ) )
		  e = nil
		  Assert.AreEqual data, clone.Decrypt( clone.Encrypt( data ) ), "After original is nil"
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h1
		Protected Function DecryptStream(enc As M_Crypto.Encrypter, encrypted As String) As String
		  var decrypted as string
		  
		  for i as integer = 0 to encrypted.Bytes step enc.BlockSize
		    decrypted = decrypted + enc.DecryptCBC( encrypted.MiddleBytes( i, enc.BlockSize ), enc.BlockSize >= ( encrypted.Bytes - i ) )
		  next
		  
		  return decrypted
		  
		End Function
	#tag EndMethod

	#tag Method, Flags = &h1
		Protected Function EncryptStream(enc As M_Crypto.Encrypter, data As String) As String
		  var encrypted as string
		  
		  for i as integer = 0 to data.Bytes step enc.BlockSize
		    encrypted = encrypted + enc.EncryptCBC( data.MiddleBytes( i, enc.BlockSize ), enc.BlockSize >= ( data.Bytes - i ) )
		  next
		  
		  return encrypted
		  
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub SimultaneousEncryptionTest()
		  dim e as M_Crypto.Encrypter = GetEncrypter( "some password" )
		  
		  if e is nil then
		    return
		  end if
		  
		  e.SetInitialVector "1234567890123456"
		  SimultaneousThreadEncrypter = e
		  
		  SimultaneousThread = new Thread
		  AddHandler SimultaneousThread.Run, WeakAddressOf SimultaneousThread_Run
		  SimultaneousThread.Run
		  
		  dim s as string = kLongData
		  dim encrypted as string
		  
		  dim l as integer = s.LenB
		  
		  while SimultaneousThreadCounter = 0
		    App.YieldToNextThread
		  wend
		  
		  Assert.Message "Main starting encryption"
		  
		  for index as integer = 1 to l step 16
		    dim block as string = s.MidB( index, 16 )
		    encrypted = encrypted + e.EncryptCBC( block, ( index + 16 ) > l )
		    App.YieldToNextThread
		  next
		  
		  Assert.Message "Main finished encryption"
		  
		  Assert.IsTrue SimultaneousThreadCounter <> 0, "SimultaneousThread did not run"
		  
		  dim decrypted as string
		  l = encrypted.LenB
		  
		  Assert.Message "Main starting decryption"
		  
		  for index as integer = 1 to l step 16
		    dim block as string = encrypted.MidB( index, 16 )
		    decrypted = decrypted + e.DecryptCBC( block, ( index + 16 ) > l )
		    while SimultaneousThread.State <> Thread.NotRunning
		      App.YieldToNextThread
		    wend
		  next
		  
		  Assert.Message "Main finished decryption"
		  
		  Assert.AreEqual s, decrypted, "Decrypted data did not match"
		  
		  while SimultaneousThread.State <> Thread.NotRunning
		    App.YieldToNextThread
		  wend
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub SimultaneousThread_Run(sender As Thread)
		  #pragma unused sender
		  
		  dim s as string = Crypto.GenerateRandomBytes( 16 )
		  
		  while s.LenB < 100000
		    s = s + s
		  wend
		  
		  s = s.LeftB( s.LenB - ( s.LenB mod 16 ) ) // Exact multiple of 16
		  
		  dim e as M_Crypto.Encrypter = SimultaneousThreadEncrypter
		  
		  dim l as integer = s.LenB
		  dim encrypted as string
		  
		  Assert.Message "Thread starting encryption"
		  
		  for index as integer = 1 to l step 16
		    if IsTimeToDie then
		      return
		    end if
		    
		    dim block as string = s.MidB( index, 16 )
		    encrypted = encrypted + e.EncryptCBC( block, ( index + 16 ) > l )
		    SimultaneousThreadCounter = SimultaneousThreadCounter + 1
		    App.YieldToNextThread
		  next
		  
		  Assert.Message "Thread finished encryption"
		  
		  dim decrypted as string
		  l = encrypted.LenB
		  
		  Assert.Message "Thread starting decryption"
		  
		  for index as integer = 1 to l step 16
		    dim block as string = encrypted.MidB( index, 16 )
		    decrypted = decrypted + e.DecryptCBC( block, ( index + 16 ) > l )
		    App.YieldToNextThread
		  next
		  
		  Assert.Message "Thread finished decryption"
		  
		  Assert.AreEqual s, decrypted, "Thread decryption failed"
		  
		End Sub
	#tag EndMethod


